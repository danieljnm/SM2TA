<?xml version="1.0" encoding="utf-8"?>
<nta>
	<declaration>
		double battery_level_ = 15;
		chan gen_SsProcessOrder_inner_start;
	</declaration>
	<template>
		<name>Tbot</name>
		<location id="StStartup" x="0" y="0">
			<name x="-15" y="15">StStartup</name>
		</location>
		<location id="gen_pre_SsProcessOrder" x="400" y="0">
			<name x="385" y="15">gen_pre_SsProcessOrder</name>
			<committed/>
		</location>
		<location id="SsProcessOrder" x="800" y="0">
			<name x="785" y="15">SsProcessOrder</name>
		</location>
		<location id="StQueue" x="1200" y="0">
			<name x="1185" y="15">StQueue</name>
		</location>
		<location id="StNavigateToBase" x="0" y="300">
			<name x="-15" y="315">StNavigateToBase</name>
		</location>
		<location id="StRecharge" x="400" y="300">
			<name x="385" y="315">StRecharge</name>
		</location>
		<location id="StBatteryDepleted" x="800" y="300">
			<name x="785" y="315">StBatteryDepleted</name>
		</location>
		<location id="StiNavigateToCoffeeMachine" x="1200" y="300">
			<name x="1185" y="315">StiNavigateToCoffeeMachine</name>
		</location>
		<location id="StiWaitForOrder" x="0" y="600">
			<name x="-15" y="615">StiWaitForOrder</name>
		</location>
		<location id="StiDeliverOrder" x="400" y="600">
			<name x="385" y="615">StiDeliverOrder</name>
		</location>
		<init ref="StStartup"/>
		<transition>
			<source ref="StStartup"/>
			<target ref="StQueue"/>
		</transition>
		<transition>
			<source ref="gen_pre_SsProcessOrder"/>
			<target ref="SsProcessOrder"/>
			<label kind="synchronisation" x="550" y="15">gen_SsProcessOrder_inner_start!</label>
		</transition>
		<transition>
			<source ref="StQueue"/>
			<target ref="StBatteryDepleted"/>
		</transition>
		<transition>
			<source ref="StQueue"/>
			<target ref="StNavigateToBase"/>
		</transition>
		<transition>
			<source ref="StQueue"/>
			<target ref="StRecharge"/>
		</transition>
		<transition>
			<source ref="StQueue"/>
			<target ref="StNavigateToBase"/>
		</transition>
		<transition>
			<source ref="StQueue"/>
			<target ref="gen_pre_SsProcessOrder"/>
		</transition>
		<transition>
			<source ref="StNavigateToBase"/>
			<target ref="StQueue"/>
		</transition>
		<transition>
			<source ref="StRecharge"/>
			<target ref="StQueue"/>
		</transition>
		<transition>
			<source ref="StiNavigateToCoffeeMachine"/>
			<target ref="StiWaitForOrder"/>
		</transition>
		<transition>
			<source ref="StiWaitForOrder"/>
			<target ref="StiDeliverOrder"/>
		</transition>
		<transition>
			<source ref="StiDeliverOrder"/>
			<target ref="StQueue"/>
		</transition>
	</template>
	<template>
		<name>SsProcessOrder_inner</name>
		<location id="gen_init" x="0" y="0">
			<name x="-15" y="15">gen_init</name>
		</location>
		<location id="StiNavigateToCoffeeMachine" x="400" y="0">
			<name x="385" y="15">StiNavigateToCoffeeMachine</name>
		</location>
		<location id="StiWaitForOrder" x="800" y="0">
			<name x="785" y="15">StiWaitForOrder</name>
		</location>
		<location id="StiDeliverOrder" x="1200" y="0">
			<name x="1185" y="15">StiDeliverOrder</name>
		</location>
		<init ref="gen_init"/>
		<transition>
			<source ref="gen_init"/>
			<target ref="StiNavigateToCoffeeMachine"/>
			<label kind="synchronisation" x="150" y="15">gen_SsProcessOrder_inner_start?</label>
		</transition>
	</template>
	<system>
		system Tbot, SsProcessOrder_inner;
	</system>
</nta>

